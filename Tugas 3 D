void display(){
glClear(GL_COLOR_BUFFER_BIT| GL_DEPTH_BUFFER_BIT);
    if(maze == true){
        glPushMatrix();
        glRotated(rotasi,a,b,c);
        layer1();
        player();
        nim();
        glPopMatrix();
    }else{
        glPushMatrix();
        glRotated(rotasi,a,b,c);
        layer2();
        player();
        nim();
        glPopMatrix();
    }

glFlush();
}

void inputan(unsigned char key,int x,int y){
    if(key == 'w' || key == 'W'){
        posy+=atas;
    }else if(key == 'a' || key == 'A'){
        posx-=kiri;
    }else if(key == 's' || key == 'S'){
        posy-=bawah;
    }else if(key == 'd' || key == 'D'){
        posx+=kanan;
    }else if(key == 'j' || key == 'J'){
        rotasi+=inc;
        rotatet=rotasi;
        a=0;
        b=1;
        c+=0.5;
    }else if(key == 'l' || key == 'L'){
        rotasi-=inc;
        rotatet=rotasi;
        a=0;
        b=1;
        c-=0.5;
    }else if(key == 'i' || key == 'I'){
        rotasi+=inc;
        rotatet=rotasi;
        a=1;
        b-=0.1;
        c+=0.5;
    }else if(key == 'k' || key == 'K'){
        rotasi-=inc;
        rotatet=rotasi;
        a=1;
        b=0;
        c-=0.5;
    }else if(key == 'v' || key == 'V'){
        countt+=1;
        if(countt%2==1){
            d=1.0;
            inc=3.0;
            rotasi = rotatet;
            a=1;
            b=0;
            c=0;
            randX = 0;
            randY = 0;
            incrotate = 0.03;
        }else if(countt%2==0){
            d=0;
            inc=0;
            rotasi = 0;
            a=0;
            b=0;
            c=0;
            incrotate = 0.0;
            theta= 0.0;
        }
    }else if(key == 'c' || key == 'C'){
        if(maze == true ){
            maze = false;
        }else{
            maze = true;
        }
        if( d==1.0 ){
            randX = 0;
            randY = 0;
        }else{
            randX = (rand()%28)+5;
            randY = (rand()%28)+5;
        }
        posx=0;
        posy=0;
    }
        // unutk tidak nabrak
    if((q+posx >= -27.5 && w+posx <= -2.5) && (e+posy == 24.5 || r+posy == 27.5) && maze == true){
        atas=0;
    }else if((q+posx >= -2.5 && w+posx <= 2.5) && (e+posy == 29.5 || r+posy == 22.5) && maze == true){
        atas=0;
    }else{
        atas=0.5;
    }

    petunjuk();
    player();
    display();
}

void myIdle(){
    theta +=incrotate;
    display();
}

void myinit(){
glMatrixMode(GL_PROJECTION);
glLoadIdentity();
glOrtho(-43.0,43.0,-43.0,43.0,43.0,-43.0);
glMatrixMode(GL_MODELVIEW);
glClearColor(1.0,1.0,1.0,1.0);
glColor3f(0.0,0.0,0.0);
glShadeModel(GL_FLAT);
glEnable(GL_DEPTH_TEST);
}
int main(int argc, char* argv[]){
glutInit(&argc,argv);
glutInitDisplayMode(GLUT_SINGLE | GLUT_RGB | GLUT_DEPTH);
glutInitWindowSize(500,500);
glutInitWindowPosition(100,100);
glutCreateWindow("Maze 3D");
glutDisplayFunc(display);
glutKeyboardFunc(inputan);
glutIdleFunc(myIdle);
myinit();
glutMainLoop();
return 0;
}
