    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(1.5+randX, 23.0+randY, d);
    glVertex3f(2.0+randX, 23.0+randY, d);
    glVertex3f(2.0+randX, 27.5+randY, d);
    glVertex3f(1.5+randX, 27.5+randY, d);
    glEnd();
    //belakang
    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(2.0+randX, 23.0+randY, -d);
    glVertex3f(1.5+randX, 23.0+randY, -d);
    glVertex3f(1.5+randX, 27.5+randY, -d);
    glVertex3f(2.0+randX, 27.5+randY, -d);
    glEnd();
    //atas
    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(1.5+randX, 27.5+randY, d);
    glVertex3f(2.0+randX, 27.5+randY, d);
    glVertex3f(2.0+randX, 27.5+randY, -d);
    glVertex3f(1.5+randX, 27.5+randY, -d);
    glEnd();
    //bawah
    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(1.5+randX, 23.0+randY, d);
    glVertex3f(2.0+randX, 23.0+randY, d);
    glVertex3f(2.0+randX, 23.0+randY, -d);
    glVertex3f(1.5+randX, 23.0+randY, -d);
    glEnd();
    //kiri
    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(1.5+randX, 27.5+randY, d);
    glVertex3f(1.5+randX, 27.5+randY, -d);
    glVertex3f(1.5+randX, 23.0+randY, -d);
    glVertex3f(1.5+randX, 23.0+randY, d);
    glEnd();
    //kanan
    glColor3f(0.0,0.0,0.0);
    glBegin(GL_POLYGON);
    glVertex3f(2.0+randX, 27.5+randY, d);
    glVertex3f(2.0+randX, 27.5+randY, -d);
    glVertex3f(2.0+randX, 23.0+randY, -d);
    glVertex3f(2.0+randX, 23.0+randY, d);
    glEnd();
}

void petunjuk(){
cout<< "nilai x1 = "<<q+posx <<endl;
cout<< "nilai y1 = "<<e+posy <<endl;
cout<< "nilai x2 = "<<w+posx <<endl;
cout<< "nilai y2 = "<<r+posy <<endl;
cout<< "================" <<endl;
}
void player(){
//////////////////////////PLAYER///////////////////////////////
//depan
glColor3f(0.0,1.0,1.0);
glBegin(GL_POLYGON);
glVertex3f(q+posx, e+posy, d);
glVertex3f(w+posx, e+posy, d);
glVertex3f(w+posx, r+posy, d);
glVertex3f(q+posx, r+posy, d);
glEnd();
//belakang
glColor3f(1.0,0.0,0.0);
glBegin(GL_POLYGON);
glVertex3f(w+posx, e+posy, -d);
glVertex3f(q+posx, e+posy, -d);
glVertex3f(q+posx, r+posy, -d);
glVertex3f(w+posx, r+posy, -d);
glEnd();
//atas
glColor3f(1.0,1.0,0.0);
glBegin(GL_POLYGON);
glVertex3f(q+posx, r+posy, d);
glVertex3f(w+posx, r+posy, d);
glVertex3f(w+posx, r+posy, -d);
glVertex3f(q+posx, r+posy, -d);
glEnd();
//bawah
glColor3f(1.0,0.0,1.0);
glBegin(GL_POLYGON);
glVertex3f(q+posx, e+posy, d);
glVertex3f(w+posx, e+posy, d);
glVertex3f(w+posx, e+posy, -d);
glVertex3f(q+posx, e+posy, -d);
glEnd();
//kiri
glColor3f(0.0,0.0,1.0);
glBegin(GL_POLYGON);
glVertex3f(q+posx, e+posy, d);
glVertex3f(q+posx, e+posy, -d);
glVertex3f(q+posx, e+posy, -d);
glVertex3f(q+posx, e+posy, d);
glEnd();
//kanan
glColor3f(0.0,1.0,0.0);
glBegin(GL_POLYGON);
glVertex3f(q+posx, r+posy, d);
glVertex3f(q+posx, r+posy, -d);
glVertex3f(q+posx, e+posy, -d);
glVertex3f(q+posx, e+posy, d);
glEnd();

}

